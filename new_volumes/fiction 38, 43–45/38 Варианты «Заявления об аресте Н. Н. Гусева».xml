    <div type="section" xml:id="h000013025002">
      <head rend="left">* № 2.</head>
      <p rend="left" id="1c1f7a36-0a08-4575-b8fb-139cc470cb2e">
        <choice>
          <reg>Так</reg>
          <orig>Такъ</orig>
        </choice>
        <choice>
          <reg>рассуждают</reg>
          <orig>разсуждаютъ</orig>
        </choice>
        <choice>
          <reg>те</reg>
          <orig>тѣ</orig>
        </choice> люди, которые за свои <choice>
          <reg>вероятно</reg>
          <orig>вѣроятно</orig>
        </choice>
        <choice>
          <reg>большия</reg>
          <orig>большiя</orig>
