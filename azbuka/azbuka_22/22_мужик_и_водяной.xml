    </reg>
   </choice>
   <choice>
    <reg>
     топор
    </reg>
   </choice>
   <choice>
    <reg>
     в
    </reg>
   </choice>
   <choice>
    <reg>
     реку
    </reg>
   </choice>
   ;
   <choice>
    <reg>
     с
    </reg>
   </choice>
   горя
   <choice>
    <reg>
     сел
    </reg>
   </choice>
