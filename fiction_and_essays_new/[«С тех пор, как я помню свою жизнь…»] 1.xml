      <choice>
       <reg>
        находил
       </reg>
       <orig>
        находилъ
       </orig>
      </choice>
      <choice>
       <reg>
        в
       </reg>
       <orig>
        въ
       </orig>
      </choice>
      <choice>
       <reg>
        них
       </reg>
       <orig>
        нихъ
       </orig>
      </choice>
